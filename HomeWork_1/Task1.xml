<mujoco model="world_with_shapes">
    <compiler coordinate="local" angle="degree" inertiafromgeom="true"/>
    <size nuserdata="1000" nconmax="500" njmax="1000"/>
    <visual>
        <map fogstart="3" fogend="5" force="0.1" zfar="10"/>
        <quality shadowsize="2048"/>
    </visual>
    <asset>
        <!-- Define materials for visual appeal -->
        <material name="mat1" rgba="0.8 0.2 0.2 1"/>
        <material name="mat2" rgba="0.2 0.2 0.8 1"/>
        <material name="mat4" rgba="0.3 0.3 0.3 1"/>
        <material name="mat3" rgba="0.5 0.5 0.5 1"/>
        <material name="mat5" rgba="1.5 0.5 0.5 1"/>
    </asset>
    <worldbody>
        <!-- Add a rectangle -->
        <body name="rectangle" pos="0 0 0">
            <geom type="box" size="0.5 3 0.5" material="mat1"/>
        </body>
        <!-- Add a globe at the end of the rectangle -->
        <body name="globe" pos="0 3 0">
            <geom type="sphere" size="1" material="mat2"/>
        </body>
        <body name="cylinder" pos="0 -3 0" euler="0 90 0">
            <geom type="cylinder" size="0.5 1" material="mat3"/>
            <!-- Rectangle blocks at both ends of the cylinder -->
            <body name="rect_block1" pos="0 -2 0.75">                <!-- Positioned at one end of the cylinder -->
                <geom type="box" size="0.5 2 0.25" material="mat4"/>
            </body>
            <body name="rect_block2" pos="0 -2 -0.75">                <!-- Positioned at the other end of the cylinder -->
                <geom type="box" size="0.5 2 0.25" material="mat4"/>
            </body>
        </body>
        <body name="cylinde1r" pos="0 -7 0" euler="90 0 90">
            <geom type="cylinder" size="0.5 0.65" material="mat3"/>
        </body>
        <body name="cylinde11r" pos="0 -8 0" euler="0 0 0">
            <geom type="cylinder" size="0.5 0.65" material="mat3"/>
        </body>
        <body name="cylinde121r" pos="-1 -8 0" euler="0 90 0">
            <geom type="cylinder" size="0.5 0.65" material="mat3"/>
        </body>
        <body name="BOX" pos="-2.5 -8 0" euler="0 90 0">
            <geom type="box" size="1 0.65 1" material="mat3"/>
        </body>
        <body name="BO1X" pos="-2.5 -8 -2" euler="0 90 0">
            <geom type="box" size="1 0.65 1.75" material="mat3"/>
        </body>
    </worldbody>
</mujoco>
